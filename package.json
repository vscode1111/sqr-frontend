{
  "name": "sqr-claim-frontend",
  "private": true,
  "version": "0.0.13",
  "scripts": {
    "commit": "git-cz",
    "dev": "cross-env NODE_ENV=local vite",
    "dev:dev": "cross-env NODE_ENV=dev vite",
    "dev:prod": "cross-env NODE_ENV=prod vite",
    "build": "tsc & cross-env NODE_ENV=local vite build",
    "build:prod": "tsc & cross-env NODE_ENV=prod vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --config ./.prettierrc --write \"src/**/*.ts*\"",
    "http-server": "http-server dist -p 5173 --proxy http://localhost:5173?"
  },
  "files": [
    "src",
    ".editorconfig",
    ".eslintignore",
    ".eslintrc.json",
    ".gitignore",
    ".prettierignore",
    ".prettierrc",
    "LICENSE",
    "package.json",
    "tsconfig.json"
  ],
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.3",
    "@mui/material": "^5.14.14",
    "@okta/okta-auth-js": "^7.5.0",
    "@okta/okta-react": "^6.7.0",
    "axios": "^1.5.1",
    "buffer": "^6.0.3",
    "mobx": "^6.10.2",
    "mobx-react": "^9.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-router-dom": "^6.17.0",
    "shamirs-secret-sharing": "^2.0.0",
    "tss-react": "^4.9.3",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.4",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.102",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "http-server": "^14.1.1",
    "prettier": "^3.0.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  }
}
